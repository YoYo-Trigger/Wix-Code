// FOR AUTHORS | Start
export function autDaySun3_change() {

let SunAutDet = $w("#autDetailsSun3");
let SunAutTyp = $w("#autDetailsSun3");
let SunAutOft = $w("#autOftenSun3");

let SunAut = $w("#autDaySun3");
let MonAut = $w("#autDayMon3");
let TueAut = $w("#autDayTue3");
let WedAut = $w("#autDayWed3");
let ThurAut = $w("#autDayThur3");
let FriAut = $w("#autDayFri3");
let SatAut = $w("#autDaySat3");

var isChecked = SunAut.checked + MonAut.checked + TueAut.checked + WedAut.checked + ThurAut.checked + FriAut.checked + SatAut.checked;

if (isChecked !== 0) {
 	console.log('At Least 1 Selected');
      SunAut.required = false;
      MonAut.required = false;
      TueAut.required = false;
	  WedAut.required = false;
	  ThurAut.required = false;
	  FriAut.required = false;
	  SatAut.required = false;
	       }
	          
else {
	console.log('None Selected');
	  SunAut.required = true;
	  MonAut.required = true;
	  TueAut.required = true;
	  WedAut.required = true;
	  ThurAut.required = true;
	  FriAut.required = true;
	  SatAut.required = true;
}

if( SunAut.checked) {
   SunAutDet.expand();
   SunAutTyp.required = true;
   SunAutOft.required = true;
     console.log('Expanding Sun Aut Details 3 \& Requiring Sun Type and Often Time');
	 
}
else {
   SunAutDet.collapse();
   SunAutTyp.required = false;
   SunAutOft.required = false;
}
}

export function autDayMon3_change() {

let MonAutDet = $w("#autDetailsMon3");
let MonAutTyp = $w("#autTypeMon3");
let MonAutOft = $w("#autOftenMon3");

let SunAut = $w("#autDaySun3");
let MonAut = $w("#autDayMon3");
let TueAut = $w("#autDayTue3");
let WedAut = $w("#autDayWed3");
let ThurAut = $w("#autDayThur3");
let FriAut = $w("#autDayFri3");
let SatAut = $w("#autDaySat3");

var isChecked = SunAut.checked + MonAut.checked + TueAut.checked + WedAut.checked + ThurAut.checked + FriAut.checked + SatAut.checked;

if (isChecked !== 0) {
 	console.log('At Least 1 Selected');
      SunAut.required = false;
      MonAut.required = false;
      TueAut.required = false;
	  WedAut.required = false;
	  ThurAut.required = false;
	  FriAut.required = false;
	  SatAut.required = false;
	       }
	          
else {
	console.log('None Selected');
	  SunAut.required = true;
	  MonAut.required = true;
	  TueAut.required = true;
	  WedAut.required = true;
	  ThurAut.required = true;
	  FriAut.required = true;
	  SatAut.required = true;
}

 if(MonAut.checked) {
   MonAutDet.expand();
   MonAutTyp.required = true;
   MonAutOft.required = true;
     console.log('Expanding Mon Aut Details 3 \& Requiring Mon Type and Often Time');
}
else {
   MonAutDet.collapse();
   MonAutTyp.required = false;
   MonAutOft.required = false;
}
