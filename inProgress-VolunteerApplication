// COMMITMENT/AVAILABILITY | Start

// **PRIMARY BOXES** | Start

// FOR ADMINS | Start
export function admDaySun1_change() {

let SunAdmDet = $w("#admDetailsSun1");
let SunAdmLen = $w("#admLengthSun1");
let SunAdmSta = $w("#admStartSun1");

let SunAdm = $w("#admDaySun1");
let MonAdm = $w("#admDayMon1");
let TueAdm = $w("#admDayTue1");
let WedAdm = $w("#admDayWed1");
let ThurAdm = $w("#admDayThur1");
let FriAdm = $w("#admDayFri1");
let SatAdm = $w("#admDaySat1");

