// POSITONS APPLYING FOR | Start

// PRIMARY POSITION | Start
export function PrimaryChoices_change() {

let PrimaryChoices = $w("#PrimaryChoices");
let SecondaryChoices = $w("#SecondaryChoices");

let admCommitment = $w("#adminCommitment1");
let autCommitment = $w("#authorCommitment1");
let artCommitment = $w("#artistCommitment1");
let vlogCommitment = $w("#vloggerCommitment1");
let strCommitment = $w("#streamerCommitment1");

let autContent = $w("#authorContent1");
let artContent = $w("#artistContent1");
let vlogContent = $w("#vloggerContent1");
let strContent = $w("#streamerContent1");

let Admin = (PrimaryChoices.value === "Admin");
let Author = (PrimaryChoices.value === "Author");
let Artist = (PrimaryChoices.value === "Artist");
let Vlogger = (PrimaryChoices.value === "Vlogger");
let Streamer = (PrimaryChoices.value === "Streamer");

/*
CURRENTLY UNREAD
let AdmDiv = $w("#admDiv1");
let AutDiv = $w("#autDiv1");
let ArtDiv = $w("#artDiv1");
let VlogDiv = $w("#vlogDiv1");
let StrDiv = $w("#strDiv1");
*/

let SunAdm = $w("#admDaySun1");
let MonAdm = $w("#admDayMon1");
let TueAdm = $w("#admDayTue1");
let WedAdm = $w("#admDayWed1");
let ThurAdm = $w("#admDayThur1");
let FriAdm = $w("#admDayFri1");
let SatAdm = $w("#admDaySat1");

let SunAut = $w("#autDaySun1");
let MonAut = $w("#autDayMon1");
let TueAut = $w("#autDayTue1");
let WedAut = $w("#autDayWed1");
let ThurAut = $w("#autDayThur1");
let FriAut = $w("#autDayFri1");
let SatAut = $w("#autDaySat1");

let SunArt = $w("#artDaySun1");
let MonArt = $w("#artDayMon1");
let TueArt = $w("#artDayTue1");
let WedArt = $w("#artDayWed1");
let ThurArt = $w("#artDayThur1");
let FriArt = $w("#artDayFri1");
let SatArt = $w("#artDaySat1");

let SunVlog = $w("#vlogDaySun1");
let MonVlog = $w("#vlogDayMon1");
let TueVlog = $w("#vlogDayTue1");
let WedVlog = $w("#vlogDayWed1");
let ThurVlog = $w("#vlogDayThur1");
let FriVlog = $w("#vlogDayFri1");
let SatVlog = $w("#vlogDaySat1");

let SunStr = $w("#strDaySun1");
let MonStr = $w("#strDayMon1");
let TueStr = $w("#strDayTue1");
let WedStr = $w("#strDayWed1");
let ThurStr = $w("#strDayThur1");
let FriStr = $w("#strDayFri1");
let SatStr = $w("#strDaySat1");

if (PrimaryChoices.value !== undefined) {

   SecondaryChoices.enable();
      console.log('Primary Position Selected | Enabling Secondary Position Selection');

}

else {

   SecondaryChoices.disable();

}

if (Admin) {

   admCommitment.expand();
      console.log('Primary Position Selected: ADMIN | Expanding ADMIN COMMITMENT CONTAINER 1');

      if (admCommitment.collapsed !== true) {
         SunAdm.required = true;
         MonAdm.required = true;
         TueAdm.required = true;
         WedAdm.required = true;
         ThurAdm.required = true;
         FriAdm.required = true;
         SatAdm.required = true;
       console.log('Requiring Admin 1 Checkboxes');
	  }
	  
}

else {
	    admCommitment.collapse();
		 SunAdm.required = false;
         MonAdm.required = false;
         TueAdm.required = false;
         WedAdm.required = false;
         ThurAdm.required = false;
         FriAdm.required = false;
         SatAdm.required = false;
   
}

if (Author) {

   autCommitment.expand();
   autContent.required = true;
      console.log('Primary Position Selected: AUTHOR | Expanding ADMIN COMMITMENT CONTAINER 1 \& Requiring authorContent1');
	  
	  if (autCommitment.collapsed !== true) {
         SunAut.required = true;
         MonAut.required = true;
         TueAut.required = true;
         WedAut.required = true;
         ThurAut.required = true;
         FriAut.required = true;
         SatAut.required = true;
       console.log('Requiring Author 1 Checkboxes');
	  }
}

else {
	   autCommitment.collapse();
	   autContent.required = false;
		 SunAut.required = false;
         MonAut.required = false;
         TueAut.required = false;
         WedAut.required = false;
         ThurAut.required = false;
         FriAut.required = false;
         SatAut.required = false;

}

if (Artist) {

   artCommitment.expand();
   artContent.required = true;
      console.log('Primary Position Selected: ARTIST | Expanding ARTIST COMMITMENT CONTAINER 1 \& Requiring artistContent1');
	  
	  if (artCommitment.collapsed !== true) {
         SunArt.required = true;
         MonArt.required = true;
         TueArt.required = true;
         WedArt.required = true;
         ThurArt.required = true;
         FriArt.required = true;
         SatArt.required = true;
       console.log('Requiring Artist 1 Checkboxes');
	  }

}
else {
	   artCommitment.collapse();
	   artContent.required = false;
		 SunArt.required = false;
         MonArt.required = false;
         TueArt.required = false;
         WedArt.required = false;
         ThurArt.required = false;
         FriArt.required = false;
         SatArt.required = false;
}

if (Vlogger) {

   vlogCommitment.expand();
   vlogContent.required = true;
      console.log('Primary Position Selected: VLOGGER | Expanding VLOGGER COMMITMENT CONTAINER 1 \& Requiring vloggerContent1');
	  
	  if (vlogCommitment.collapsed !== true) {
         SunVlog.required = true;
         MonVlog.required = true;
         TueVlog.required = true;
         WedVlog.required = true;
         ThurVlog.required = true;
         FriVlog.required = true;
         SatVlog.required = true;
       console.log('Requiring Vlogger 1 Checkboxes');
	  }

}
else {
	   vlogCommitment.collapse();
	   vlogContent.required = false;
		 SunVlog.required = false;
         MonVlog.required = false;
         TueVlog.required = false;
         WedVlog.required = false;
         ThurVlog.required = false;
         FriVlog.required = false;
         SatVlog.required = false;
}

if (Streamer) {

   strCommitment.expand();
   strContent.required = true;
      console.log('Primary Position Selected: STREAMER | Expanding STREAMER COMMITMENT CONTAINER 1 \& Requiring streamerContent1');
	  
	  if (strCommitment.collapsed !== true) {
         SunStr.required = true;
         MonStr.required = true;
         TueStr.required = true;
         WedStr.required = true;
         ThurStr.required = true;
         FriStr.required = true;
         SatStr.required = true;
       console.log('Requiring Streamer 1 Checkboxes');
	  }

}
else {
	   strCommitment.collapse();
	   strContent.required = false;
		 SunStr.required = false;
         MonStr.required = false;
         TueStr.required = false;
         WedStr.required = false;
         ThurStr.required = false;
         FriStr.required = false;
         SatStr.required = false;
}

}
// PRIMARY POSITION | End


// SECONDARY POSITION | Start
export function SecondaryChoices_change() {

let SecondaryChoices = $w("#SecondaryChoices");
let TertiaryChoices = $w("#TertiaryChoices");

let admCommitment = $w("#adminCommitment2");

let artCommitment = $w("#artistCommitment2");
let vlogCommitment = $w("#vloggerCommitment2");
let strCommitment = $w("#streamerCommitment2");

let autContent = $w("#authorContent2");
let artContent = $w("#artistContent2");
let vlogContent = $w("#vloggerContent2");
let strContent = $w("#streamerContent2");

let None = (SecondaryChoices.value === "None");
let Admin = (SecondaryChoices.value === "Admin");
let Author = (SecondaryChoices.value === "Author");
let Artist = (SecondaryChoices.value === "Artist");
let Vlogger = (SecondaryChoices.value === "Vlogger");
let Streamer = (SecondaryChoices.value === "Streamer");

/*
CURRENTLY UNREAD
let AdmDiv = $w("#admDiv2");
let AutDiv = $w("#autDiv2");
let ArtDiv = $w("#artDiv2");
let VlogDiv = $w("#vlogDiv2");
let StrDiv = $w("#strDiv2");
*/

let SunAdm = $w("#admDaySun2");
let MonAdm = $w("#admDayMon2");
let TueAdm = $w("#admDayTue2");
let WedAdm = $w("#admDayWed2");
let ThurAdm = $w("#admDayThur2");
let FriAdm = $w("#admDayFri2");
let SatAdm = $w("#admDaySat2");

let SunAut = $w("#autDaySun2");
let MonAut = $w("#autDayMon2");
let TueAut = $w("#autDayTue2");
let WedAut = $w("#autDayWed2");
let ThurAut = $w("#autDayThur2");
let FriAut = $w("#autDayFri2");
let SatAut = $w("#autDaySat2");

let SunArt = $w("#artDaySun2");
let MonArt = $w("#artDayMon2");
let TueArt = $w("#artDayTue2");
let WedArt = $w("#artDayWed2");
let ThurArt = $w("#artDayThur2");
let FriArt = $w("#artDayFri2");
let SatArt = $w("#artDaySat2");

let SunVlog = $w("#vlogDaySun2");
let MonVlog = $w("#vlogDayMon2");
let TueVlog = $w("#vlogDayTue2");
let WedVlog = $w("#vlogDayWed2");
let ThurVlog = $w("#vlogDayThur2");
let FriVlog = $w("#vlogDayFri2");
let SatVlog = $w("#vlogDaySat2");

let SunStr = $w("#strDaySun2");
let MonStr = $w("#strDayMon2");
let TueStr = $w("#strDayTue2");
let WedStr = $w("#strDayWed2");
let ThurStr = $w("#strDayThur2");
let FriStr = $w("#strDayFri2");
let SatStr = $w("#strDaySat2");

if (SecondaryChoices.value !== "None") {

   TertiaryChoices.enable();
      console.log('Secondary Position Selected | Enabling Secondary Position Selection');

}

else {

   TertiaryChoices.disable();

}

if (Admin) {

   admCommitment.expand();
      console.log('Secondary Position Selected: ADMIN | Expanding ADMIN COMMITMENT CONTAINER 2');

      if (admCommitment.collapsed !== true) {
         SunAdm.required = true;
         MonAdm.required = true;
         TueAdm.required = true;
         WedAdm.required = true;
         ThurAdm.required = true;
         FriAdm.required = true;
         SatAdm.required = true;
       console.log('Requiring Admin 2 Checkboxes');
	  }
	  
}

else {
	    admCommitment.collapse();
		 SunAdm.required = false;
         MonAdm.required = false;
         TueAdm.required = false;
         WedAdm.required = false;
         ThurAdm.required = false;
         FriAdm.required = false;
         SatAdm.required = false;
   
}

if (Author) {
let autCommitment = $w("#authorCommitment2");

   autCommitment.expand();
   autContent.required = true;
      console.log('Secondary Position Selected: AUTHOR | Expanding ADMIN COMMITMENT CONTAINER 2 \& Requiring authorContent2');
	  
	  if (autCommitment.collapsed !== true) {
         SunAut.required = true;
         MonAut.required = true;
         TueAut.required = true;
         WedAut.required = true;
         ThurAut.required = true;
         FriAut.required = true;
         SatAut.required = true;
       console.log('Requiring Author 2 Checkboxes');
	  }
}

else {
	   autCommitment.collapse();
	   autContent.required = false;
		 SunAut.required = false;
         MonAut.required = false;
         TueAut.required = false;
         WedAut.required = false;
         ThurAut.required = false;
         FriAut.required = false;
         SatAut.required = false;

}

if (Artist) {

   artCommitment.expand();
   artContent.required = true;
      console.log('Secondary Position Selected: ARTIST | Expanding ARTIST COMMITMENT CONTAINER 2 \& Requiring artistContent2');
	  
	  if (artCommitment.collapsed !== true) {
         SunArt.required = true;
         MonArt.required = true;
         TueArt.required = true;
         WedArt.required = true;
         ThurArt.required = true;
         FriArt.required = true;
         SatArt.required = true;
       console.log('Requiring Artist 2 Checkboxes');
	  }

}
else {
	   artCommitment.collapse();
	   artContent.required = false;
		 SunArt.required = false;
         MonArt.required = false;
         TueArt.required = false;
         WedArt.required = false;
         ThurArt.required = false;
         FriArt.required = false;
         SatArt.required = false;
}

if (Vlogger) {

   vlogCommitment.expand();
   vlogContent.required = true;
      console.log('Secondary Position Selected: VLOGGER | Expanding VLOGGER COMMITMENT CONTAINER 2 \& Requiring vloggerContent2');
	  
	  if (vlogCommitment.collapsed !== true) {
         SunVlog.required = true;
         MonVlog.required = true;
         TueVlog.required = true;
         WedVlog.required = true;
         ThurVlog.required = true;
         FriVlog.required = true;
         SatVlog.required = true;
       console.log('Requiring Vlogger 2 Checkboxes');
	  }

}
else {
	   vlogCommitment.collapse();
	   vlogContent.required = false;
		 SunVlog.required = false;
         MonVlog.required = false;
         TueVlog.required = false;
         WedVlog.required = false;
         ThurVlog.required = false;
         FriVlog.required = false;
         SatVlog.required = false;
}

if (Streamer) {

   strCommitment.expand();
   strContent.required = true;
      console.log('Secondary Position Selected: STREAMER | Expanding STREAMER COMMITMENT CONTAINER 2 \& Requiring streamerContent2');
	  
	  if (strCommitment.collapsed !== true) {
         SunStr.required = true;
         MonStr.required = true;
         TueStr.required = true;
         WedStr.required = true;
         ThurStr.required = true;
         FriStr.required = true;
         SatStr.required = true;
       console.log('Requiring Streamer 2 Checkboxes');
	  }

}
else {
	   strCommitment.collapse();
	   strContent.required = false;
		 SunStr.required = false;
         MonStr.required = false;
         TueStr.required = false;
         WedStr.required = false;
         ThurStr.required = false;
         FriStr.required = false;
         SatStr.required = false;
}

}
