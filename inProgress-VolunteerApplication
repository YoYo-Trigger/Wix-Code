if (isChecked !== 0) {
 	console.log('At Least 1 Selected');
      SunVlog.required = false;
      MonVlog.required = false;
      TueVlog.required = false;
	  WedVlog.required = false;
	  ThurVlog.required = false;
	  FriVlog.required = false;
	  SatVlog.required = false;
	       }
	          
else {
	console.log('None Selected');
	  SunVlog.required = true;
	  MonVlog.required = true;
	  TueVlog.required = true;
	  WedVlog.required = true;
	  ThurVlog.required = true;
	  FriVlog.required = true;
	  SatVlog.required = true;
}

 if(WedVlog.checked) {
   WedVlogOft.expand();
   WedVlogOft.required = true;
     console.log('Expanding Wed Vlog Details 3 \& Requiring Wed Often');
}
else {
   WedVlogOft.collapse();
   WedVlogOft.required = false;
} 
}

