// PRIMARY POSITION | Start
export function PrimaryChoices_change() {

let PrimaryChoices = $w("#PrimaryChoices");
let SecondaryChoices = $w("#SecondaryChoices");

let admCommitment = $w("#adminCommitment1");
let autCommitment = $w("#authorCommitment1");
let artCommitment = $w("#artistCommitment1");
let vlogCommitment = $w("#vloggerCommitment1");
let strCommitment = $w("#streamerCommitment1");

let autContent = $w("#authorContent1");
let artContent = $w("#artistContent1");
let vlogContent = $w("#vloggerContent1");
let strContent = $w("#streamerContent1");

let Admin = (PrimaryChoices.value === "Admin");
let Author = (PrimaryChoices.value === "Author");
let Artist = (PrimaryChoices.value === "Artist");
let Vlogger = (PrimaryChoices.value === "Vlogger");
let Streamer = (PrimaryChoices.value === "Streamer");

/*
