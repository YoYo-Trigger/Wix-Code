// COMMITMENT/AVAILABILITY | Start

// **PRIMARY BOXES** | Start

// FOR ADMINS | Start
export function admDaySun1_change() {
if( $w("#admDaySun1").checked) {
   $w("#admDetailsSun1").expand();
   $w("#admLengthSun1").required = true;
   $w("#admStartSun1").required = true;
}
else {
   $w("#admDetailsSun1").collapse();
   $w("#admLengthSun1").required = false;
   $w("#admStartSun1").required = false;
}
}

