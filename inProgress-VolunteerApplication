// FOR AUTHORS | Start
export function autDaySun1_change() {
if( $w("#autDaySun1").checked) {
   $w("#autDetailsSun1").expand();
   $w("#autTypeSun1").required = true;
   $w("#autOftenSun1").required = true;
}
else {
   $w("#autDetailsSun1").collapse();
   $w("#autTypeSun1").required = false;
   $w("#autOftenSun1").required = false;
}
}

export function autDayMon1_change() {
 if( $w("#autDayMon1").checked) {
