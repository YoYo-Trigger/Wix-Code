let TueVlog = $w("#vlogDayTue3");
let WedVlog = $w("#vlogDayWed3");
let ThurVlog = $w("#vlogDayThur3");
let FriVlog = $w("#vlogDayFri3");
let SatVlog = $w("#vlogDaySat3");

