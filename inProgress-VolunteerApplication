let AutDiv = $w("#autDiv3");
let ArtDiv = $w("#artDiv3");
let VlogDiv = $w("#vlogDiv3");
let StrDiv = $w("#strDiv3");
*/

let SunAdm = $w("#admDaySun3");
let MonAdm = $w("#admDayMon3");
let TueAdm = $w("#admDayTue3");
let WedAdm = $w("#admDayWed3");
let ThurAdm = $w("#admDayThur3");
let FriAdm = $w("#admDayFri3");
let SatAdm = $w("#admDaySat3");

let SunAut = $w("#autDaySun3");
let MonAut = $w("#autDayMon3");
let TueAut = $w("#autDayTue3");
let WedAut = $w("#autDayWed3");
let ThurAut = $w("#autDayThur3");
let FriAut = $w("#autDayFri3");
let SatAut = $w("#autDaySat3");

let SunArt = $w("#artDaySun3");
let MonArt = $w("#artDayMon3");
let TueArt = $w("#artDayTue3");
let WedArt = $w("#artDayWed3");
let ThurArt = $w("#artDayThur3");
let FriArt = $w("#artDayFri3");
let SatArt = $w("#artDaySat3");

