// TERTIARY POSITION | Start
export function TertiaryChoices_change() {

let TertiaryChoices = $w("#TertiaryChoices");

let admCommitment = $w("#adminCommitment3");
let autCommitment = $w("#authorCommitment3");
let artCommitment = $w("#artistCommitment3");
let vlogCommitment = $w("#vloggerCommitment3");
let strCommitment = $w("#streamerCommitment3");

let autContent = $w("#authorContent3");
let artContent = $w("#artistContent3");
let vlogContent = $w("#vloggerContent3");
let strContent = $w("#streamerContent3");

let Admin = (TertiaryChoices.value === "Admin");
let Author = (TertiaryChoices.value === "Author");
let Artist = (TertiaryChoices.value === "Artist");
let Vlogger = (TertiaryChoices.value === "Vlogger");
let Streamer = (TertiaryChoices.value === "Streamer");

/*
CURRENTLY UNREAD
let AdmDiv = $w("#admDiv3");
let AutDiv = $w("#autDiv3");
let ArtDiv = $w("#artDiv3");
let VlogDiv = $w("#vlogDiv3");
let StrDiv = $w("#strDiv3");
*/

let SunAdm = $w("#admDaySun3");
let MonAdm = $w("#admDayMon3");
let TueAdm = $w("#admDayTue3");
let WedAdm = $w("#admDayWed3");
let ThurAdm = $w("#admDayThur3");
let FriAdm = $w("#admDayFri3");
let SatAdm = $w("#admDaySat3");

let SunAut = $w("#autDaySun3");
let MonAut = $w("#autDayMon3");
let TueAut = $w("#autDayTue3");
let WedAut = $w("#autDayWed3");
let ThurAut = $w("#autDayThur3");
let FriAut = $w("#autDayFri3");
let SatAut = $w("#autDaySat3");

let SunArt = $w("#artDaySun3");
let MonArt = $w("#artDayMon3");
let TueArt = $w("#artDayTue3");
let WedArt = $w("#artDayWed3");
let ThurArt = $w("#artDayThur3");
let FriArt = $w("#artDayFri3");
let SatArt = $w("#artDaySat3");

let SunVlog = $w("#vlogDaySun3");
let MonVlog = $w("#vlogDayMon3");
let TueVlog = $w("#vlogDayTue3");
let WedVlog = $w("#vlogDayWed3");
let ThurVlog = $w("#vlogDayThur3");
let FriVlog = $w("#vlogDayFri3");
let SatVlog = $w("#vlogDaySat3");

let SunStr = $w("#strDaySun3");
let MonStr = $w("#strDayMon3");
let TueStr = $w("#strDayTue3");
let WedStr = $w("#strDayWed3");
let ThurStr = $w("#strDayThur3");
let FriStr = $w("#strDayFri3");
let SatStr = $w("#strDaySat3");


if (Admin) {

   admCommitment.expand();
      console.log('Tertiary Position Selected: ADMIN | Expanding ADMIN COMMITMENT CONTAINER 3');

      if (admCommitment.collapsed !== true) {
         SunAdm.required = true;
         MonAdm.required = true;
         TueAdm.required = true;
         WedAdm.required = true;
         ThurAdm.required = true;
         FriAdm.required = true;
         SatAdm.required = true;
       console.log('Requiring Admin 3 Checkboxes');
	  }
	  
}

else {
	    admCommitment.collapse();
		 SunAdm.required = false;
         MonAdm.required = false;
         TueAdm.required = false;
         WedAdm.required = false;
         ThurAdm.required = false;
         FriAdm.required = false;
         SatAdm.required = false;
   
}

if (Author) {

   autCommitment.expand();
   autContent.required = true;
      console.log('Tertiary Position Selected: AUTHOR | Expanding ADMIN COMMITMENT CONTAINER 3 \& Requiring authorContent3');
	  
	  if (autCommitment.collapsed !== true) {
         SunAut.required = true;
         MonAut.required = true;
         TueAut.required = true;
         WedAut.required = true;
         ThurAut.required = true;
         FriAut.required = true;
         SatAut.required = true;
       console.log('Requiring Author 3 Checkboxes');
	  }
}

else {
