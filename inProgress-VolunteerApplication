// FOR ARTISTS | Start
export function artDaySun2_change() {

let SunArtDet = $w("#artDetailsSun2");
let SunArtTyp = $w("#artDetailsSun2");
let SunArtOft = $w("#artOftenSun2");

let SunArt = $w("#artDaySun2");
let MonArt = $w("#artDayMon2");
let TueArt = $w("#artDayTue2");
let WedArt = $w("#artDayWed2");
let ThurArt = $w("#artDayThur2");
let FriArt = $w("#artDayFri2");
let SatArt = $w("#artDaySat2");

var isChecked = SunArt.checked + MonArt.checked + TueArt.checked + WedArt.checked + ThurArt.checked + FriArt.checked + SatArt.checked;

if (isChecked !== 0) {
 	console.log('At Least 1 Selected');
      SunArt.required = false;
      MonArt.required = false;
      TueArt.required = false;
	  WedArt.required = false;
	  ThurArt.required = false;
	  FriArt.required = false;
	  SatArt.required = false;
	       }
	          
else {
	console.log('None Selected');
	  SunArt.required = true;
	  MonArt.required = true;
	  TueArt.required = true;
	  WedArt.required = true;
	  ThurArt.required = true;
	  FriArt.required = true;
	  SatArt.required = true;
}

if( SunArt.checked) {
   SunArtDet.expand();
   SunArtTyp.required = true;
   SunArtOft.required = true;
     console.log('Expanding Sun Art Details 2 \& Requiring Sun Type and Often Time');
}
else {
   SunArtDet.collapse();
   SunArtTyp.required = false;
   SunArtOft.required = false;
}
}

