// FOR STREAMERS | Start
export function strDaySun2_change() {

let SunStrDet = $w("#strDetailsSun2");
let SunStrLen = $w("#strDetailsSun2");
let SunStrSta = $w("#strStartSun2");

let SunStr = $w("#strDaySun2");
let MonStr = $w("#strDayMon2");
let TueStr = $w("#strDayTue2");
let WedStr = $w("#strDayWed2");
let ThurStr = $w("#strDayThur2");
let FriStr = $w("#strDayFri2");
let SatStr = $w("#strDaySat2");

var isChecked = SunStr.checked + MonStr.checked + TueStr.checked + WedStr.checked + ThurStr.checked + FriStr.checked + SatStr.checked;

if (isChecked !== 0) {
 	console.log('At Least 1 Selected');
      SunStr.required = false;
      MonStr.required = false;
      TueStr.required = false;
	  WedStr.required = false;
	  ThurStr.required = false;
	  FriStr.required = false;
	  SatStr.required = false;
	       }
	          
else {
	console.log('None Selected');
	  SunStr.required = true;
	  MonStr.required = true;
	  TueStr.required = true;
	  WedStr.required = true;
	  ThurStr.required = true;
	  FriStr.required = true;
	  SatStr.required = true;
}

if( SunStr.checked) {
   SunStrDet.expand();
   SunStrLen.required = true;
   SunStrSta.required = true;
     console.log('Expanding Sun Str Details 2 \& Requiring Sun Length and Start Time');
}
else {
   SunStrDet.collapse();
   SunStrLen.required = false;
   SunStrSta.required = false;
}
}

export function strDayMon2_change() {

let MonStrDet = $w("#strDetailsMon2");
let MonStrLen = $w("#strLengthMon2");
let MonStrSta = $w("#strStartMon2");

let SunStr = $w("#strDaySun2");
let MonStr = $w("#strDayMon2");
let TueStr = $w("#strDayTue2");
let WedStr = $w("#strDayWed2");
let ThurStr = $w("#strDayThur2");
let FriStr = $w("#strDayFri2");
let SatStr = $w("#strDaySat2");

var isChecked = SunStr.checked + MonStr.checked + TueStr.checked + WedStr.checked + ThurStr.checked + FriStr.checked + SatStr.checked;

if (isChecked !== 0) {
 	console.log('At Least 1 Selected');
      SunStr.required = false;
      MonStr.required = false;
      TueStr.required = false;
	  WedStr.required = false;
	  ThurStr.required = false;
	  FriStr.required = false;
	  SatStr.required = false;
	       }
	          
else {
	console.log('None Selected');
	  SunStr.required = true;
	  MonStr.required = true;
	  TueStr.required = true;
	  WedStr.required = true;
	  ThurStr.required = true;
	  FriStr.required = true;
	  SatStr.required = true;
}

 if(MonStr.checked) {
   MonStrDet.expand();
   MonStrLen.required = true;
   MonStrSta.required = true;
     console.log('Expanding Mon Str Details 2 \& Requiring Mon Length and Start Time');
}
else {
   MonStrDet.collapse();
   MonStrLen.required = false;
   MonStrSta.required = false;
}
}

